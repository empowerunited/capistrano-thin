pid: <%= thin_pid %>
tag: <%= thin_tag %>
user: <%= thin_user %>
group: <%= thin_group %>

<% if thin_use_unix == 'true' %>
socket: <%= thin_socket %>
<% else %>
address: <%= thin_address %>
port: <%= thin_port %>
<% end %>

onebyone: true

threaded: <%= thin_threaded %>
no-epoll: <%= thin_no_epoll %>
daemonize: true
timeout: 30
wait: 30

log: <%= thin_log %>
max_conns: <%= thin_max_conns %>
require: []

environment: <%= retrieve_env %>
max_persistent_conns: <%= thin_max_persistent_conns %>
servers: <%= thin_servers %>
chdir: <%= current_path %>
